<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CreditCheckServiceService WSDL service -->

  <service name="CreditCheckService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservices.sai"/>
      <parameter name="wsdlServiceElement" value="CreditCheckServiceService"/>
      <parameter name="schemaQualified" value="http://webservices.sai"/>
      <parameter name="wsdlServicePort" value="CreditCheckService"/>
      <parameter name="className" value="sai.webservices.CreditCheckServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CreditCheckService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="checkCredit" qname="operNS:checkCredit" xmlns:operNS="http://webservices.sai" returnQName="retNS:checkCreditReturn" xmlns:retNS="http://webservices.sai" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:ssn" xmlns:pns="http://webservices.sai" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="checkCredit"/>

  </service>
</deployment>
